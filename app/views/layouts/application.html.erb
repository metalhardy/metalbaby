<!DOCTYPE html>
<html>
<head>
  <title>Metalbaby</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div class="header">
		<div class="container">
			<%= render 'layouts/header' %>
		</div>
	</div>
	<div id="wrap" class="content">
		<div id="main">
			<div class="container">
				<%= yield %>
			</div>
		</div>
	</div>
	<div class="footer">
		<div class="container">
			<%= render 'layouts/footer' %>
		</div>
	</div>

</body>
</html>
